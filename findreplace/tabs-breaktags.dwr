<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="allOpenDocuments">
    <qtext qname="&lt;br&gt;\s*((&lt;strong&gt;)?(&lt;em&gt;)?)(&nbsp;){5,}\s*" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="</p>\n<p>$1" param2=""/>
</dwquery>
