<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="allOpenDocuments">
    <qtext qname="&lt;p&gt;(&lt;strong&gt;)?(&lt;em&gt;)?(&nbsp;)+ ?([^&lt;]+)(&lt;/em&gt;)?(&lt;/strong&gt;)?" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<p>$1$2$4$5$6" param2=""/>
</dwquery>
