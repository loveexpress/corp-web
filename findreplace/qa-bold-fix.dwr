<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="allOpenDocuments">
    <qtext qname="&lt;p(.*)class=&quot;qa&quot;&gt;&lt;(b|strong)&gt;([^&lt;]+)&lt;/(b|strong)&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<p$1class=&quot;qa&quot;>$3" param2=""/>
</dwquery>
