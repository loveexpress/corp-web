<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="allOpenDocuments">
    <qtext qname="(&lt;p[^&gt;]*?&gt;(&lt;strong&gt;)?(&lt;em&gt;)?)(&nbsp;)+\s?" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$1" param2=""/>
</dwquery>
